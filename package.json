{
  "name": "my_react_starter_kit",
  "version": "1.0.0",
  "description": "Simple Starter kit [webpack,react,react-router,recompose,lodash,emotion]",
  "main": "index.js",
  "scripts": {
    "lint": "eslint **/*.{js,jsx} --quiet",
    "format": "prettier --write --list-different --single-quote --tab-width=4 --print-width=120 webpack.config.js \"src/**/*.{js,jsx}\"",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "prebuild": "rimraf dist",
    "start": "webpack-dev-server --env.dev --hot --open",
    "build": "webpack -p --env.prod",
    "build:dev": "webpack --env.dev",
    "test": "test"
  },
  "keywords": [
    "react",
    "react-router",
    "recompose",
    "emotion"
  ],
  "author": "Aigars Beinarovics",
  "license": "ISC",
  "dependencies": {
    "emotion": "^7.2.0",
    "lodash": "^4.17.4",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-flexbox-grid": "^1.1.5",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.25.0",
    "theming": "^1.1.0",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.28.7",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "mobx-react-devtools": "^4.2.15",
    "npm": "^5.4.0",
    "prettier": "^1.5.3",
    "prettier-eslint": "^6.4.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.7.1"
  }
}
